{
    "name": "jonathanraftery/bullhorn-auth-client",
    "description": "Simple authorization client to create Bullhorn REST API sessions",
    "keywords": [
        "bullhorn", "authentication", "rest", "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan Raftery",
            "email": "jonathan@jonathanraftery.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "league/oauth2-client": "^2.3",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/database": "^9.26"
    },
    "autoload": {
        "classmap": [
            "src/",
            "Models/"
        ],
        "psr-4": {
            "jonathanraftery\\Bullhorn\\REST\\Authentication\\": "src/",
            "Models\\" : "Models/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    }
}
